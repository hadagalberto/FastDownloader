                    MaterialOpacity="0.55" />
			</ExperimentalAcrylicBorder.Material>
		</ExperimentalAcrylicBorder>
        <Grid ColumnDefinitions="230,1,5*,4*" VerticalAlignment="Top">
		    <Button Content="Login no Spotify"
                    Grid.Column="0"
                    Margin="5"></Button>
            <TextBox 
                AcceptsReturn="True"
                Margin="5"
                Grid.Column="1"
                Width="180"
                >
                Teste
            </TextBox>
        </Grid>
        
	</Panel>
</Window>
